@{
    ViewBag.CurrentMenu = "SoftwareVersion";
}

<div class="row">
    <div class="box box-primary">
        <div class="box-header with-border">
            <h3 class="box-title">版本列表</h3>
            <div class="pull-right box-tools">
                <button id="btnAdd" title="" class="btn btn-primary" type="button" data-original-title="新增功能" data-toggle="tooltip" data-widget="">
                    <i class="fa fa-plus-circle"></i>&nbsp;&nbsp;新增
                </button>
            </div>
        </div>
        <div class="box-body">
            <table class="table table-striped table-bordered table-hover dataTable" role="grid">
                <thead>
                    <tr role="row">
                        <th tabindex="0" aria-label="">
                            版本名称
                        </th>
                        <th tabindex="0" aria-label="">
                            预计开始时间（开发）
                        </th>
                        <th tabindex="0" aria-label="">
                            预计结束时间（开发）
                        </th>
                        <th tabindex="0" aria-label="">
                            预计发布时间
                        </th>
                        <th tabindex="0" aria-label="">
                            实际发布时间
                        </th>
                        <th tabindex="0" aria-label="">
                            备注
                        </th>
                        <th tabindex="0" style="width: 230px;" aria-label="">
                            操作
                        </th>
                    </tr>
                </thead>
                <tbody id="tableBody"></tbody>
            </table>
        </div>
        <div class="pull-right">
            <ul id="pagination"></ul>
        </div>
    </div>
</div>
@Html.Partial("Edit")
@section scripts{
    <script src="~/js/softwareversion.js"></script>
}
